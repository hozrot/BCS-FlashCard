<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bcsflashcard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BCSFLASHCARD">
        <activity
            android:name=".Syllabus"
            android:label="@string/title_activity_syllabus"></activity>
        <activity android:name=".FlashCardList" />
        <activity android:name=".QuizList" />
        <activity android:name=".ExamResult" />
        <activity android:name=".Quiz" />
        <activity android:name=".FlashCardAnswer" />
        <activity android:name=".FlashCard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>